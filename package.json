{
  "name": "blog-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "start:en": "npm run start -- --locale en",
    "build": "docusaurus build --config docusaurus.config.ts",
    "build:agc": "docusaurus build  --config docusaurus.config.agc.ts",
    "build:srffix": "docusaurus build & npm run suffix",
    "build:en": "npm run build -- --locale en",
    "write:en": "npm run write-translations -- --locale en",
    "swizzle": "docusaurus swizzle",
    "suffix": "node ./scripts/suffix.js",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "upload-oss": "node -r dotenv/config ./scripts/upload-oss.js dotenv_config_path=./.env",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "3.0.1",
    "@docusaurus/preset-classic": "3.0.1",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "@hw-agconnect/auth": "^1.5.1",
    "@mdx-js/react": "^3.0.0",
    "@popperjs/core": "^2.11.8",
    "antd": "^5.12.5",
    "axios": "^1.6.7",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-popper": "^2.3.0",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.0.1",
    "@docusaurus/tsconfig": "3.0.1",
    "@docusaurus/types": "3.0.1",
    "cos-nodejs-sdk-v5": "^2.15.4",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.2",
    "glob": "^11.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^16.2.6",
    "typescript": "~5.2.2"
  },
  "lint-staged": {
    "**/*.{md,mdx}": [
      "node -r dotenv/config ./scripts/upload-oss.js dotenv_config_path=./.env",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
